{
  "name": "tailblazor",
  "version": "0.10",
  "description": "template for using tailwind css in jit mode with blazor projects",
  "devDependencies": {
    "postcss-import": "^14.0.1",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.1.2",
    "tailwindcss-debug-screens": "^2.2.0"
  },
  "scripts": {
    "build": "npx tailwindcss --postcss postcss.config.js -i TailBlazor.css -o ./wwwroot/TailBlazor.min.css",
    "watch": "npx tailwindcss --watch --postcss postcss.config.js -i TailBlazor.css -o ./wwwroot/TailBlazor.min.css",
    "publish": "npx tailwindcss --minify --postcss postcss.config.js -i TailBlazor.css -o ./wwwroot/TailBlazor.min.css"
  },
  "keywords": [
    "tailwindcss",
    "blazor"
  ],
  "author": "mcnerdius",
  "license": "0BSD",
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
