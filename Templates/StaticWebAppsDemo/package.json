{
  "name": "tailblazor",
  "version": "0.9.1",
  "description": "template for using tailwind css in jit mode with blazor projects",
  "devDependencies": {
    "postcss-import": "^14.0.1",
    "tailwindcss": "^3.0.0-alpha.1"
  },
  "scripts": {
    "build-wasm": "npx tailwindcss --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorWasm/wwwroot/site.min.css",
    "watch-wasm": "npx tailwindcss --watch --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorWasm/wwwroot/site.min.css",
    "publish-wasm": "npx tailwindcss --minify --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorWasm/wwwroot/site.min.css",
    "build-server": "npx tailwindcss --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorServer/wwwroot/site.min.css",
    "watch-server": "npx tailwindcss --watch --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorServer/wwwroot/site.min.css",
    "publish-server": "npx tailwindcss --minify --config tailwind.config.js --postcss postcss.config.js -i site.css -o ./BlazorServer/wwwroot/site.min.css"
  },
  "keywords": [
    "tailwindcss",
    "blazor"
  ],
  "author": "mcnerdius",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.2.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}